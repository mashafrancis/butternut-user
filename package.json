{
  "name": "butternut-user",
  "version": "0.0.1",
  "description": "Butternut backend micro-service for user API",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mashafrancis/butternut-user.git"
  },
  "author": "Francis Masha <francismasha96@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nodemon --exec ts-node src/index.ts",
    "start:dev": "node -r ts-node/register -r dotenv-safe/config src/index.ts",
    "cli": "node -r dotenv-safe/config -r ts-node/register src/cli.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test:unit": "jest -u --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migrations": "typeorm migration:generate -n Migration"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/core": "^6.10.14",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/microservices": "^6.10.14",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/swagger": "^4.1.10",
    "@types/uuid": "^3.4.6",
    "apollo-server-express": "^2.9.16",
    "argon2": "^0.25.1",
    "async": "^3.1.0",
    "async-exit-hook": "^2.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "googleapis": "^46.0.0",
    "graphql": "^14.5.8",
    "graphql-tag-pluck": "^0.8.7",
    "graphql-tools": "^4.0.6",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.21.3",
    "mongodb": "^3.4.1",
    "nestjs-command": "^1.3.0",
    "nodemailer": "^6.4.2",
    "passport": "^0.4.1",
    "passport-facebook-token": "^3.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.17.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "swagger-ui-express": "^4.1.2",
    "twing": "^2.2.3",
    "typeorm": "^0.2.22",
    "uuid": "^3.3.3",
    "voucher-code-generator": "^1.1.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.13.3",
    "@nestjs/schematics": "^6.8.2",
    "@nestjs/testing": "^6.10.14",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.9",
    "@types/graphql": "^14.5.0",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^24.0.25",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/luxon": "^1.21.0",
    "@types/mongodb": "^3.3.14",
    "@types/node": "^13.1.6",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/parser": "^2.15.0",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-jsdoc-rules": "^0.2.0",
    "typescript": "^3.7.4"
  }
}
