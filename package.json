{
  "name": "butternut-user",
  "version": "0.0.1",
  "description": "Butternut backend micro-service for user API",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mashafrancis/butternut-user.git"
  },
  "author": "Francis Masha <francismasha96@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nodemon --exec ts-node src/index.ts",
    "start:dev": "node -r ts-node/register -r dotenv-safe/config src/index.ts",
    "cli": "node -r dotenv-safe/config -r ts-node/register src/cli.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:unit": "jest -u --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migrations": "typeorm migration:generate -n Migration"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "@nestjs/graphql": "^10.0.10",
    "@nestjs/microservices": "^8.4.4",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/swagger": "^5.2.1",
    "@types/uuid": "^8.3.4",
    "apollo-server-express": "^3.6.7",
    "argon2": "^0.28.5",
    "async": "^3.2.3",
    "async-exit-hook": "^2.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.3",
    "faker": "^6.6.6",
    "googleapis": "^100.0.0",
    "graphql": "^16.3.0",
    "graphql-tag-pluck": "^0.8.7",
    "graphql-tools": "^8.2.8",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^2.3.2",
    "mongodb": "^4.5.0",
    "nestjs-command": "^3.1.1",
    "nodemailer": "^6.7.3",
    "passport": "^0.5.2",
    "passport-facebook-token": "^4.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "rxjs-compat": "^6.6.7",
    "swagger-ui-express": "^4.3.0",
    "twing": "^5.1.1",
    "typeorm": "^0.3.6",
    "uuid": "^8.3.2",
    "voucher-code-generator": "^1.3.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.10",
    "@nestjs/testing": "^8.4.4",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.8",
    "@types/graphql": "^14.5.0",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/luxon": "^2.3.1",
    "@types/mongodb": "^4.0.6",
    "@types/node": "^17.0.25",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  }
}
